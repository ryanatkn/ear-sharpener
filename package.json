    "lodash": "^4.6.1",
    "normalize.css": "^4.0.0",
    "react": "^15.0.0",
    "react-addons-shallow-compare": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-localstorage": "^0.4.1",
    "redux-thunk": "^2.0.1"
